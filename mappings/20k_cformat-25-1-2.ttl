@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@base <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .
@prefix ex: <http://example.com/> .

<#Employee> a rr:TriplesMap ;
	rml:logicalSource [ rml:source ".//app//source//data//veracity25.csv";
																					rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://example.com/Mutation/{cFormat}";
		rr:class ex:employee
	];
	rr:predicateObjectMap [
		rr:predicate ex:HGNC_ID;
		rr:objectMap [
			rml:reference "HGNC ID";
		]
	];
	rr:predicateObjectMap [
		rr:predicate ex:Mutation_genome_position;
		rr:objectMap [
			rml:reference "Mutation genome position";
		]
	];
	rr:predicateObjectMap [
		rr:predicate ex:GENOMIC_MUTATION_ID;
		rr:objectMap [
			rml:reference "GENOMIC_MUTATION_ID";
		]
	]
	.