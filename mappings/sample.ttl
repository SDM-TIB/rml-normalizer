@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@base <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .
@prefix mThesis: <http://mThesis.eu/vocab/> .

<#Employee> a rr:TriplesMap ;
	rml:logicalSource [ rml:source ".//app//source//data//48000_data.csv";
																					rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://master-thesis/Employee/{B}";
		rr:class mThesis:employee
	];
	rr:predicateObjectMap [
		rr:predicate mThesis:emp_F;
		rr:objectMap [
			rml:reference "F";
		]
	];
	rr:predicateObjectMap [
		rr:predicate mThesis:emp_G;
		rr:objectMap [
			rml:reference "G";
		]
	];
	rr:predicateObjectMap [
		rr:predicate mThesis:emp_H;
		rr:objectMap [
			rml:reference "H";
		]
	].