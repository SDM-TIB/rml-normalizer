@base <http://tib.de/ontario/mapping#> .
@prefix ex: <http://example.com/> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .

<#Example> a rr:TriplesMap ;
    rml:logicalSource [ rml:referenceFormulation ql:CSV ;
            rml:source ".//app//experiments//csv//mapping--data-CT_B.csv" ] ;
    rr:predicateObjectMap [ rr:objectMap [ rr:joinCondition [ rr:child "C" ;
                            rr:parent "C" ] ;
                    rr:parentTriplesMap <#D> ] ;
            rr:predicate ex:attr_d ],
        [ rr:objectMap [ rr:joinCondition [ rr:child "C" ;
                            rr:parent "C" ] ;
                    rr:parentTriplesMap <#E> ] ;
            rr:predicate ex:attr_e ],
        [ rr:objectMap [ rml:reference "C" ] ;
            rr:predicate ex:attr_c ] ;
    rr:subjectMap [ rr:class ex:attr_b ;
            rr:template "http://example.com/{B}" ] .

<#D> a rr:TriplesMap ;
    rml:logicalSource [ rml:referenceFormulation ql:CSV ;
            rml:source ".//app//experiments//csv//mapping--data-PT_C_D.csv" ] ;
    rr:subjectMap [ rr:template "{D}" ] .

<#E> a rr:TriplesMap ;
    rml:logicalSource [ rml:referenceFormulation ql:CSV ;
            rml:source ".//app//experiments//csv//mapping--data-PT_C_E.csv" ] ;
    rr:subjectMap [ rr:template "{E}" ] .

